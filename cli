#! /usr/bin/php

<?php

require('lib/system.config.php');
require('lib/system.functions.php');
require('lib/cli.functions.php');

autoload_plugin_assets();

$plugin = $argv[1];
$script = $argv[2];

require('plugins/'.$plugin.'/cli/'.$script.'.php');

if($success)
{
  success($success);
}

if($error)
{
  error($error);
}

echo "\n";
